{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-elfinder": "^0.3.12",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "jeremykenedy/laravel-logger": "^2.2",
        "jeremykenedy/laravel-users": "^3.1",
        "laravel/framework": "5.6.*",
        "laravel/nova": "*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.4.0",
        "maatwebsite/laravel-nova-excel": "^1.0",
        "runlinenl/nova-profile-tool": "^1.1",
        "spatie/laravel-cors": "^1.2",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-model-status": "^1.6",
        "spatie/laravel-permission": "^2.12",
        "spatie/nova-backup-tool": "^1.0",
        "tymon/jwt-auth": "^1.0.0-rc.2",
        "vyuldashev/nova-spatie-permission": "^1.3"
    },
    "repositories": [
    {
            "type": "path",
            "url": "./nova"
        }
    ],
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "dev-master",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
